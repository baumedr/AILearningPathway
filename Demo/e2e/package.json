{
  "name": "todo-app-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for the Todo Application",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen http://localhost:4173",
    "install:browsers": "playwright install",
    "install:deps": "playwright install-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "todo",
    "react",
    "typescript"
  ],
  "author": "Todo App Team",
  "license": "MIT"
}